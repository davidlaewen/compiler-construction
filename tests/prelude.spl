// SPL Prelude

printBool(b) :: Bool -> Void {
  if (b) {
    print('T');
    print('r');
    print('u');
    print('e');
  } else {
    print('F');
    print('a');
    print('l');
    print('s');
    print('e');
  }
  return;
}

printTuple(t) :: (a,b) -> Void {
  print('(');
  print(t.fst);
  print(',');
  print(t.snd);
  print(')');
}
