main() {
  // Should print 42 nine times
  print( (42,'a').fst );
  print( (True, ('c',42)).snd.snd );
  print( ( (True,False), (42, (1,'c')) ).snd.fst );
  print( ( ('a',(3,4)), (1,(42,False)) ).snd.snd.fst );
  print( ( ('c',42), (False,3) ).fst.snd );
  print( (True,40).snd + (2,'c').fst );
  print( 50 - ('c',(8,False)).snd.fst );
  print( (21 * 2, False).fst );
  print( ('a', 2 + 20 * 2).snd );
}
