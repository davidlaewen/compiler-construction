data MaybeBool {
  None(), Just(val:Bool)
}

data Colour {
  Red(), Green(), Blue()
}

data IntList {
  Nil(), Cons(head:Int,tail:IntList)
}

col2Char(col) {
  if (isRed(col)) {
    return 'R';
  } else {
    if (isGreen(col)) {
      return 'G';
    } else {
      return 'B';
    }
  }
}

printIntList(l) {
  if (isNil(l)) {
    print('[');
    print(']');
  } else {
    print(l.head);
    print(':');
    printIntList(l.tail);
  }
  return;
}

